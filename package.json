{
	"name": "@okou/unstorage-map-set-kv",
	"version": "1.1.0",
	"author": "okou",
	"license": "MIT-0",
	"type": "module",
	"scripts": {
		"build": "tsup --clean --dts --sourcemap --format esm,cjs src/index.ts",
		"fix": "biome check --apply src test",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"prepack": "run-p typecheck build"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"keywords": [
		"storage",
		"collection",
		"unjs",
		"unstorage"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ookkoouu/unstorage-map-set-kv.git"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.0",
		"lefthook": "^1.6.10",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.2.5",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"destr": "^2.0.3",
		"fast-deep-equal": "^3.1.3",
		"unstorage": "^1.10.2"
	}
}
